% Initialise the mapping table and fetch the particular implementation being used.

#ori             r7,  r0,  (0,1,2)
lli             r15, 0x1200

setchmapi       2,   r15                ; core 1 = map 2

#ori             r0,  r0,  (0,0,3)       ; claim core 1's port

or              r0,  r10, r0    -> 2    ; send cache configuration to core 1
fetchr.eop      0x0100             ; fetch main program
