% Connect to memory.
ori                 r5,  r0,  (13,0)
setchmap            r5,  1
ori                 r0,  r0,  (0,2)  > 1

addui               r2,  r0,  1337          ; The number to store.

% Send a stream of stores to memory to make sure none go missing.
% Note that reading this instruction packet has the side effect of testing
% many consecutive read operations.
stw                 r2,  r0,  0      > 1
stw                 r2,  r0,  4      > 1
stw                 r2,  r0,  8      > 1
stw                 r2,  r0,  12     > 1
stw                 r2,  r0,  16     > 1
stw                 r2,  r0,  20     > 1
stw                 r2,  r0,  24     > 1
stw                 r2,  r0,  28     > 1
stw                 r2,  r0,  32     > 1
stw                 r2,  r0,  36     > 1
stw                 r2,  r0,  40     > 1
stw                 r2,  r0,  44     > 1
stw                 r2,  r0,  48     > 1
stw                 r2,  r0,  52     > 1
stw                 r2,  r0,  56     > 1
stw                 r2,  r0,  60     > 1
stw                 r2,  r0,  64     > 1
stw                 r2,  r0,  68     > 1
stw                 r2,  r0,  72     > 1
stw                 r2,  r0,  76     > 1
stw                 r2,  r0,  80     > 1
stw                 r2,  r0,  84     > 1
stw                 r2,  r0,  88     > 1
